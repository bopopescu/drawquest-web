<?xml version='1.0' encoding='UTF-8'?>
<project>
  <actions/>
  <description></description>
  <logRotator>
    <daysToKeep>14</daysToKeep>
    <numToKeep>-1</numToKeep>
    <artifactDaysToKeep>-1</artifactDaysToKeep>
    <artifactNumToKeep>-1</artifactNumToKeep>
  </logRotator>
  <keepDependencies>false</keepDependencies>
  <properties>
    <jenkins.plugins.hipchat.HipChatNotifier_-HipChatJobProperty>
      <room></room>
      <startNotification>false</startNotification>
    </jenkins.plugins.hipchat.HipChatNotifier_-HipChatJobProperty>
  </properties>
  <scm class="hudson.scm.NullSCM"/>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector">
    <hudson.triggers.TimerTrigger>
      <spec>*/4 * * * *</spec>
    </hudson.triggers.TimerTrigger>
  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command>#!/bin/bash
fab --parallel --roles production cloudwatch_metric:FreeMemory,&apos;free | grep &quot;cache:&quot; | sed -r &quot;s/\s+/ /g&quot; | cut -d &quot; &quot; -f4&apos;
fab --parallel --roles production cloudwatch_metric:FreeResidentMemory,&apos;free | grep &quot;Mem:&quot; | sed -r &quot;s/\s+/ /g&quot; | cut -d &quot; &quot; -f4&apos;
fab --parallel --roles production cloudwatch_metric:FreeRootDiskPercent,&apos;echo $((100-`df | grep -E &quot;/dev/(s|xv)da1&quot; | sed -r &quot;s/\s+/ /g&quot; | cut -d &quot; &quot; -f5 | cut -d &quot;%&quot; -f1`))&apos;,unit=Percent</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers/>
  <buildWrappers>
    <hudson.plugins.build__timeout.BuildTimeoutWrapper>
      <timeoutMinutes>4</timeoutMinutes>
      <failBuild>true</failBuild>
    </hudson.plugins.build__timeout.BuildTimeoutWrapper>
  </buildWrappers>
</project>